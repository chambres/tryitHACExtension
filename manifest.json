{
  "name": "plugin name",
  "version": "0",
  "description": "What do I do as an extension",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_popup": "background.html",
    "default_title": "My custom google Extension"
  },
  "web_accessible_resources": [{
    "resources": ["script.js"],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "background.js"],
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "storage"
]
}